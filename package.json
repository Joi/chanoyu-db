{
  "name": "ito-collection",
  "private": true,
  "version": "1.3.0",
  "packageManager": "pnpm@9.7.0",
  "scripts": {
    "dev": "next dev --turbo",
    "dev:local": "env-cmd -f .env.local.dev next dev --turbo",
    "dev:prod": "env-cmd -f .env.local next dev --turbo",
    "build": "NEXT_DISABLE_LIGHTNINGCSS=1 next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "build:prod": "NODE_ENV=production NEXT_DISABLE_LIGHTNINGCSS=1 next build",
    "test": "vitest run --environment=jsdom",
    "ci": "pnpm lint && pnpm typecheck && pnpm test && pnpm build:prod && vercel build --yes",
    "ci:local": "pnpm lint && pnpm typecheck && pnpm test && NEXT_DISABLE_LIGHTNINGCSS=1 pnpm build:prod",
    "prepare": "husky",
    "seed": "ts-node scripts/seed-object.ts",
    "ingest:notion": "ts-node --files --compiler-options '{\"module\":\"CommonJS\",\"moduleResolution\":\"node\"}' scripts/ingest-notion.ts",
    "seed:licenses": "ts-node scripts/seed-licenses.ts",
    "sync:accounts-schema": "ts-node scripts/sync-accounts-schema.ts",
    "backfill:tokens": "ts-node --files --compiler-options '{\"module\":\"CommonJS\",\"moduleResolution\":\"node\"}' scripts/backfill-tokens.ts",
    "migrate:location-media": "ts-node --files --compiler-options '{\"module\":\"CommonJS\",\"moduleResolution\":\"node\"}' scripts/migrate-location-media.ts",
    "cleanup:location-folder": "ts-node --files --compiler-options '{\"module\":\"CommonJS\",\"moduleResolution\":\"node\"}' scripts/cleanup-location-folder.ts",
    "clone:data": "ts-node --files --compiler-options '{\"module\":\"CommonJS\",\"moduleResolution\":\"node\"}' scripts/clone-supabase-data.ts"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.45.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "file-type": "^19.6.0",
    "heic-convert": "^1.2.4",
    "jose": "^5.10.0",
    "lucide-react": "^0.542.0",
    "nanoid": "^5.0.7",
    "next": "^14.2.32",
    "next-themes": "^0.4.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sharp": "^0.33.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.0.0",
    "@testing-library/react": "^16.0.0",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.21",
    "env-cmd": "^11.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.32",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lightningcss": "^1.25.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "supabase": "^2.34.3",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vercel": "^39.4.2",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.24.4"
  }
}
